<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.food.delivery.store.mapper.StoreMapper">

    <select id="findStoresByOwner"
            parameterType="long"
            StoreEntity="com.food.delivery.store.entity.Store">

        SELECT
            store_id,
            owner_id,
            store_name,
            store_location,
            phone,
            created_at
        FROM store
        WHERE owner_id = #{ownerId}

    </select>

</mapper>